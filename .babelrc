{
    "presets": [
        "module:metro-react-native-babel-preset"
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "@ant-design/react-native",
            }
        ],
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "extensions": [
                    ".ios.js",
                    ".android.js",
                    ".js",
                    ".json"
                ],
                "alias": {
                    "@/src": "./src",
                },
            }
        ]
    ]
}